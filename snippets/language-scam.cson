# https://github.com/atom/language-gfm/blob/master/snippets/gfm.cson

'.source.scam':
  'Method documentation':
    'prefix': 'doc'
    'body': '@@ ${1:method} - ${2:description}'
  'if':
    'prefix': 'if'
    'body': 'if (${1:#t}) \n\t$2 \n\t$3 \n'
  'cond':
    'prefix': 'cond'
    'body': 'cond \n\t((${1:#t}) \n\t\t$2 \n\t) \n\t(else \n\t$3 \n\t)'
